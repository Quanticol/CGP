<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated cgp -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="eu.quanticol.cgp.model.presentation.CGPModelWizardID"
            name="%_UI_CGPModelWizard_label"
            class="eu.quanticol.cgp.model.presentation.CGPModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/CGPModelFile.gif">
         <description>%_UI_CGPModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated cgp -->
      <editor
            id="eu.quanticol.cgp.model.presentation.CGPEditorID"
            name="%_UI_CGPEditor_label"
            icon="icons/full/obj16/CGPModelFile.gif"
            extensions="cgp"
            class="eu.quanticol.cgp.model.presentation.CGPEditor"
            contributorClass="eu.quanticol.cgp.model.presentation.CGPActionBarContributor">
      </editor>
   </extension>

</plugin>
